{
  "name": "dontuknowpe",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "api-azure"
  ],
  "scripts": {
    "dev:web": "npm --workspace apps/web run dev",
    "dev:api": "npm --workspace api-azure run start",
    "dev": "concurrently -n WEB,API -c blue,green \"npm run dev:web\" \"npm run dev:api\"",
    "build": "npm run build:web",
    "build:web": "npm --workspace apps/web run build",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.5",
    "concurrently": "^8.2.2",
    "prettier": "^3.3.3"
  }
}
